{
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "files.exclude": {
        "**/.classpath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/.factorypath": true
    },
    "workbench.iconTheme": "material-icon-theme",
    "editor.formatOnPaste": true,
    "workbench.colorTheme": "Dracula Soft",
    "editor.fontFamily": "Fira Code",
    "editor.fontSize": 17,
    "editor.lineHeight": 1.3,
    "code-runner.clearPreviousOutput": true,
    "code-runner.showExecutionMessage": false,
    "code-runner.saveFileBeforeRun": true,
    "code-runner.terminalRoot": "/mnt/",
    "tabnine.experimentalAutoImports": true,
    "terminal.integrated.defaultProfile.windows": "Git Bash",
    "workbench.startupEditor": "none",
    "liveshare.presence": true,
    "window.zoomLevel": -1,
    "explorer.copyRelativePathSeparator": "/",
    "editor.bracketPairColorization.enabled": true,
    "editor.guides.bracketPairs": "active",
    "workbench.colorCustomizations": {
        "editorBracketHighlight.foreground1": "#5fa3fd",
        "editorBracketHighlight.foreground2": "#f76666",
        "editorBracketHighlight.foreground3": "#5ffd64",
        "editorBracketHighlight.foreground4": "#e0fd5f",
        "editorBracketHighlight.foreground5": "#fda35f",
        "editorBracketHighlight.foreground6": "#c35ffd",

        "editorBracketHighlight.unexpectedBracket.foreground": "#963323"
    },
    "gitlens.codeLens.enabled": false,
    "editor.tabSize": 2,
    "editor.detectIndentation": false,
    "indentRainbow.colors": [
      "rgba(227, 201, 201, 0.1)",
      "rgba(110, 127, 128, 0.1)",
      "rgba(0, 91, 150, 0.1)",
      "rgba(100, 151, 177, 0.1)",
      "rgba(179, 205, 224, 0.1)"
    ]
}

